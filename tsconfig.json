{
  "compilerOptions": {
    "module": "CommonJS",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "allowJs": true,
    "incremental": true,
    "target": "es2020",
    "noImplicitAny": true,
    "resolveJsonModule":true,
    "noEmit": true,
    "moduleResolution": "node",
    "outDir": "dist"
  },
  "exclude": [ "node_modules","dist" ]
}